{
  "name": "Eris Boiler Documentation",
  "theme": "default",
  "ignoreCompilerErrors": "false",
  "preserveConstEnums": "true",
  "mode": "modules",
  "out": "docs",
  "readme": "./README.md",
  "exclude": [
    "**/node_modules/**",
    "**/__tests__/**"
  ],
  "excludePrivate": true,
  "external-modulemap": ".*packages\/((@[^\/]+\/)?[^\/]+)\/"
}
